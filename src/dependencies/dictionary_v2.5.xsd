<?xml version = "1.0" encoding = "UTF-8"?>
<xs:schema targetNamespace="http://api.vetrf.ru/schema/cdm/dictionary/v2"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:dt="http://api.vetrf.ru/schema/cdm/dictionary/v2"
           xmlns:bs="http://api.vetrf.ru/schema/cdm/base"
           xmlns:cl="http://api.vetrf.ru/schema/cdm/codelist/v2"
           elementFormDefault="qualified" version="2.5">

   <xs:import namespace="http://api.vetrf.ru/schema/cdm/base" schemaLocation="base_v2.1.xsd"/>
   <xs:import namespace="http://api.vetrf.ru/schema/cdm/codelist/v2" schemaLocation="codelist_v2.5r4.xsd"/>

   <!-- ELEMENT DEFINITIONS -->
   <xs:element name="purpose" type="dt:Purpose"/>
   <xs:element name="purposeList" type="dt:PurposeList"/>
   <xs:element name="unit" type="dt:Unit"/>
   <xs:element name="unitList" type="dt:UnitList"/>
   <xs:element name="country" type="dt:Country"/>
   <xs:element name="region" type="dt:Region"/>
   <xs:element name="district" type="dt:District"/>
   <xs:element name="countryList" type="dt:CountryList"/>
   <xs:element name="regionList" type="dt:RegionList"/>
   <xs:element name="districtList" type="dt:DistrictList"/>
   <xs:element name="localityList" type="dt:LocalityList"/>
   <xs:element name="streetList" type="dt:StreetList"/>
   <xs:element name="countryGuid" type="bs:UUID"/>
   <xs:element name="regionGuid" type="bs:UUID"/>
   <xs:element name="districtGuid" type="bs:UUID"/>
   <xs:element name="localityGuid" type="bs:UUID"/>
   <xs:element name="businessEntity" type="dt:BusinessEntity"/>
   <xs:element name="businessEntityList" type="dt:BusinessEntityList"/>
   <xs:element name="businessMember" type="dt:BusinessMember"/>
   <xs:element name="producer" type="dt:BusinessMember"/>
   <xs:element name="globalID" type="dt:GLNType"/>
   <xs:element name="enterpriseGuid" type="bs:UUID"/>
   <xs:element name="enterprise" type="dt:Enterprise"/>
   <xs:element name="enterpriseList" type="dt:EnterpriseList"/>
   <xs:element name="enterpriseGroup" type="dt:EnterpriseGroup"/>
   <xs:element name="productType" type="dt:ProductType"/>
   <xs:element name="productGuid" type="bs:UUID"/>
   <xs:element name="subProductGuid" type="bs:UUID"/>
   <xs:element name="product" type="dt:Product"/>
   <xs:element name="productList" type="dt:ProductList"/>
   <xs:element name="subProduct" type="dt:SubProduct"/>
   <xs:element name="subProductList" type="dt:SubProductList"/>
   <xs:element name="productItem" type="dt:ProductItem"/>
   <xs:element name="productItemList" type="dt:ProductItemList"/>
   <xs:element name="disease" type="dt:AnimalDisease"/>
   <xs:element name="diseaseList" type="dt:AnimalDiseaseList"/>
   <xs:element name="cargoType" type="dt:SubProduct"/>
   <xs:element name="r13nConditionList" type="dt:RegionalizationConditionList"/>
   <xs:element name="r13nRegionStatusList" type="dt:RegionalizationRegionStatusList"/>
   <xs:element name="r13nShippingRuleList" type="dt:RegionalizationShippingRuleList"/>
   <xs:element name="r13nZone" type="dt:Area"/>
   <xs:element name="researchMethod" type="dt:ResearchMethod"/>
   <xs:element name="researchMethodList" type="dt:ResearchMethodList"/>
   <xs:element name="activityLocationList" type="dt:ActivityLocationList"/>
   <xs:element name="businessMemberList" type="dt:BusinessMemberList"/>
   <xs:element name="certifiedProducer" type="dt:CertifiedProducer"/>
   <xs:element name="certifiedProducerList" type="dt:CertifiedProducerList"/>
   <xs:element name="borderVetControlPost" type="dt:BorderVetControlPost"/>
   <xs:element name="borderVetControlPostList" type="dt:BorderVetControlPostList"/>
   <xs:element name="storagePoint" type="dt:StoragePoint"/>
   <xs:element name="storagePointList" type="dt:StoragePointList"/>
   <xs:element name="animalPlaceKind" type="dt:AnimalPlaceKind"/>
   <xs:element name="supervisedObject" type="dt:SupervisedObject"/>
   <xs:element name="supervisedObjectList" type="dt:SupervisedObjectList"/>
   <xs:element name="supervisedObjectTypeList" type="dt:SupervisedObjectTypeList"/>
   <xs:element name="supervisedObjectActivity" type="dt:SupervisedObjectActivity"/>
   <xs:element name="supervisedObjectActivityList" type="dt:SupervisedObjectActivityList"/>
   <xs:element name="lookupScope" type="dt:RegistryLookupScope"/>
   <xs:element name="fishingLocation" type="dt:FishingLocation"/>
   <xs:element name="veterinaryLaboratoryList" type="dt:VeterinaryLaboratoryList"/>
   <xs:element name="animalSpecies" type="dt:AnimalSpecies"/>
   <xs:element name="animalSpeciesList" type="dt:AnimalSpeciesList"/>
   <xs:element name="animalBreed" type="dt:AnimalBreed"/>
   <xs:element name="animalBreedList" type="dt:AnimalBreedList"/>
   <xs:element name="animalKeepingType" type="dt:AnimalKeepingType"/>
   <xs:element name="animalKeepingTypeList" type="dt:AnimalKeepingTypeList"/>
   <xs:element name="animalKeepingPurpose" type="dt:AnimalKeepingPurpose"/>
   <xs:element name="animalKeepingPurposeList" type="dt:AnimalKeepingPurposeList"/>
   <xs:element name="animalMarkingLocation" type="dt:AnimalMarkingLocation"/>
   <xs:element name="animalMarkingLocationList" type="dt:AnimalMarkingLocationList"/>

   <!-- TYPE DEFINITIONS -->
   <xs:complexType name="Purpose">
      <xs:annotation>
         <xs:documentation>Тип, описывающий направление груза.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Наименование направления.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="forSubstandard" type="xs:boolean" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Назначение для некачественных грузов.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="intendedUse" type="dt:IntendedUse" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Назначение груза, к которому относится данное направление.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Unit">
      <xs:annotation>
         <xs:documentation>Тип, описывающий единицу измерения.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Наименование единицы измерения.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="fullName" type="bs:String255" minOccurs="0"
                           maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Полное наименование единицы измерения, если оно отличается
                        от name.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="commonUnitGuid" type="bs:UUID" minOccurs="0"
                           maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Глобальный идентификатор базовой единицы измерения.
                        Например, для центнера, тонны, тыс.тонн базовой единицей измерения является
                        килограмм.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="factor" type="xs:integer" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Коэффициент относительно базовой единицы измерения.
                        Например, для тонны этот коэффициент равен 1000, т.к. в 1 тонне 1000 кг.
                        Для килограмма этот коэффициент равен единице.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="nameEn" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Наименование единицы измерения (англ.)</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="PackingType">
      <xs:annotation>
         <xs:documentation>Вид упаковки</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="globalID" type="dt:PackingCodeType" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Уникальный идентификатор упаковки.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Наименование упаковки.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="PurposeList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список целей.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="purpose" type="dt:Purpose" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="UnitList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список единиц измерений.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="unit" type="dt:Unit" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="PackageLevelType">
      <xs:annotation>
         <xs:documentation>Тип, описывающий уровень упаковки.</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="cl:PackageLevelTypeContentType">
            <xs:attribute name="listID" type="xs:token" use="optional">
               <xs:annotation>
                  <xs:documentation>Идентификатор списка возможных значений (справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="listAgencyID" type="xs:token" use="optional">
               <xs:annotation>
                  <xs:documentation>Идентификатор организации-держателя справочника</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="listName" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Наименование списка возможных значений (справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="listVersionID" type="xs:token" use="optional">
               <xs:annotation>
                  <xs:documentation>Версия списка возможных значений (справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="name" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Наименование соответствующего объекта (элемента справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>

   <xs:complexType name="PackingCodeType">
      <xs:annotation>
         <xs:documentation>Тип для представления Packing Code в соответствии с
            "ЕК 013-2010 (ред.1) - Классификатор видов груза, упаковки и упаковочных материалов".
            См. https://eomi.eaeunion.org/ru/#/repository/nsi/173
         </xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="cl:PackingCodeTypeContentType">
            <xs:attribute name="listID" type="xs:token" use="optional">
               <xs:annotation>
                  <xs:documentation>Идентификатор списка возможных значений (справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="listAgencyID" type="xs:token" use="optional">
               <xs:annotation>
                  <xs:documentation>Идентификатор организации-держателя справочника</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="listName" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Наименование списка возможных значений (справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="listVersionID" type="xs:token" use="optional">
               <xs:annotation>
                  <xs:documentation>Версия списка возможных значений (справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="name" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Наименование соответствующего объекта (элемента справочника)</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>

   <xs:complexType name="Country">
      <xs:annotation>
         <xs:documentation>Тип, описывающий страну.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="name" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Название страны.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="fullName" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Полное название страны, если имеется.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="englishName" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Английское название страны.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="code" type="dt:Code" minOccurs="0"
                           maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Двухбуквенный код страны, соответствующий
                        стандарту ISO 3166-1.
                        В качестве кода используются буквы
                        латинского алфавита.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="code3" type="dt:Code3" minOccurs="0"
                           maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Трехбуквенный код страны, соответсвующий
                        стандарту ISO 3166-1.
                        В качестве кода используются буквы
                        латинского алфавита.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="group" type="dt:CountryGroup" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Группы, в состав которых входит страна.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="FederalDistrict">
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="fullName" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Полное название (содержить "федеральный
                        округ").
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="shortName" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Короткое название (без "федеральный округ").
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="abbreviation" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Аббревиатура.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="AddressObjectView">
      <xs:annotation>
         <xs:documentation>Обертка для адресных объектов различного уровня
            (начиная с регионов страны, заканчивая улицами населенных пунктов)
         </xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="name" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Название объекта (не содержит название типа
                        объекта).
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="englishName" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Английское название объекта.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="view" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Представление объекта (обычно это название +
                        название типа объекта).
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="regionCode" type="xs:string"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Код региона. Например, 33 - Владимирский регион
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="type" type="bs:String255"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Название типа адресного объекта (например,
                        область, район или город).
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="countryGuid" type="bs:UUID"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Глобальный идентификатор страны, к которой
                        относится данный объект.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="hasStreets" type="xs:boolean"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Флаг, указывающий наличие улиц.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Region">
      <xs:annotation>
         <xs:documentation>Тип региона страны (обычно первый уровень в
            адресной структуре).
         </xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="dt:AddressObjectView">
            <xs:sequence>
               <xs:element name="isoCode" type="dt:ISOSubDivCode" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Код региона в соответствии с ISO 3166-2
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="District">
      <xs:annotation>
         <xs:documentation>Тип района региона (обычно второй уровень в
            адресной структуре).
         </xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="dt:AddressObjectView">
            <xs:sequence>
               <xs:element name="regionGuid" type="bs:UUID"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Глобальный идентификатор рениона, к которому
                        относится данный район.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Locality">
      <xs:annotation>
         <xs:documentation>Тип населенного пункта (город, поселок, деревня и прочие).</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="dt:AddressObjectView">
            <xs:sequence>
               <xs:element name="regionGuid" type="bs:UUID"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Глобальный идентификатор рениона, к которому
                        относится данный населенный пункт.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="districtGuid" type="bs:UUID"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Глобальный идентификатор района, к которому
                        относится данный населенный пункт.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="cityGuid" type="bs:UUID"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Глобальный идентификатор города, к которому
                        относится данный населенный пункт.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Street">
      <xs:annotation>
         <xs:documentation>Тип улицы.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="dt:AddressObjectView">
            <xs:sequence>
               <xs:element name="localityGuid" type="bs:UUID"
                           minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Глобальный идентификатор насленного пункта, к которому
                        относится данная улица.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Address">
      <xs:sequence>
         <xs:element name="country" type="dt:Country" minOccurs="0" maxOccurs="1"/>
         <xs:element name="federalDistrict" type="dt:FederalDistrict" minOccurs="0" maxOccurs="1"/>
         <xs:element name="region" type="dt:Region" minOccurs="0" maxOccurs="1"/>
         <xs:element name="district" type="dt:District" minOccurs="0" maxOccurs="1"/>
         <xs:element name="locality" type="dt:Locality" minOccurs="0" maxOccurs="1"/>
         <xs:element name="subLocality" type="dt:Locality" minOccurs="0" maxOccurs="1"/>
         <xs:element name="street" type="dt:Street" minOccurs="0" maxOccurs="1"/>
         <xs:element name="house" type="bs:String255" minOccurs="0" maxOccurs="1"/>
         <xs:element name="building" type="bs:String255" minOccurs="0" maxOccurs="1"/>
