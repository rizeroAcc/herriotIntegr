<?xml version = "1.0" encoding = "UTF-8"?>
<xs:schema targetNamespace="http://api.vetrf.ru/schema/cdm/herriot/applications/v1"
           xmlns:hrt="http://api.vetrf.ru/schema/cdm/herriot/applications/v1"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:bs="http://api.vetrf.ru/schema/cdm/base"
           xmlns:apl="http://api.vetrf.ru/schema/cdm/application"
           xmlns:dt="http://api.vetrf.ru/schema/cdm/dictionary/v2"
           xmlns:vd="http://api.vetrf.ru/schema/cdm/mercury/vet-document/v2"
           elementFormDefault="qualified" version="1.0-beta">

    <xs:import namespace="http://api.vetrf.ru/schema/cdm/base" schemaLocation="base_v2.1.xsd"/>
    <xs:import namespace="http://api.vetrf.ru/schema/cdm/application" schemaLocation="application_v2.1.xsd"/>
    <xs:import namespace="http://api.vetrf.ru/schema/cdm/dictionary/v2" schemaLocation="dictionary_v2.5.xsd"/>
    <xs:import namespace="http://api.vetrf.ru/schema/cdm/mercury/vet-document/v2" schemaLocation="document_v2.5.xsd"/>
    <xs:import namespace="http://api.vetrf.ru/schema/cdm/codelist/v2" schemaLocation="codelist_v2.5r4.xsd"/>

    <!-- Application Elements -->

    <!-- ===============================================================================-->
    <!-- AMS.HRT.1 | Операции по работе с учетной карточкой животного (группы животных) -->
    <!-- ===============================================================================-->

    <!-- AMS.HRT.1.1 | Регистрация животного или группы животных -->
    <xs:element name="registerAnimalRequest" type="hrt:RegisterAnimalRequest"/>
    <xs:element name="registerAnimalResponse" type="hrt:RegisterAnimalResponse"/>

    <!-- AMS.HRT.1.2 | Внесение изменений в основные сведения о зарегистрированном животном или группе животных -->
    <xs:element name="modifyAnimalRegistrationRequest" type="hrt:ModifyAnimalRegistrationRequest"/>
    <xs:element name="modifyAnimalRegistrationResponse" type="hrt:ModifyAnimalRegistrationResponse"/>

    <!-- AMS.HRT.1.3 | Регистрация сведений о перемещении / смене собственника зарегистрированного животного или групп животных -->
    <xs:element name="registerAnimalMovementEventRequest" type="hrt:RegisterAnimalMovementEventRequest"/>
    <xs:element name="registerAnimalMovementEventResponse" type="hrt:RegisterAnimalMovementEventResponse"/>

    <!-- AMS.HRT.1.4 | Прекращение регистрации (выбытие) животного или группы животных -->
    <xs:element name="terminateAnimalRegistrationRequest" type="hrt:TerminateAnimalRegistrationRequest"/>
    <xs:element name="terminateAnimalRegistrationResponse" type="hrt:TerminateAnimalRegistrationResponse"/>

    <!-- AMS.HRT.1.5 | Аннулирование сведений о регистрации животного или группы животных -->
    <xs:element name="withdrawAnimalRegistrationRequest" type="hrt:WithdrawAnimalRegistrationRequest"/>
    <xs:element name="withdrawAnimalRegistrationResponse" type="hrt:WithdrawAnimalRegistrationResponse"/>

    <!-- AMS.HRT.1.6 | Получение основных актуальных сведений
                       о регистрации животного или группы животных по глобальному идентификатору -->
    <xs:element name="getAnimalRegistrationByGuidRequest" type="hrt:GetAnimalRegistrationByGuidRequest"/>
    <xs:element name="getAnimalRegistrationByGuidResponse" type="hrt:GetAnimalRegistrationByGuidResponse"/>

    <!-- AMS.HRT.1.7 | Получение полных актуальных сведений (включая ветпрофмероприятия)
                       о регистрации животного или группы животных по глобальному идентификатору -->
    <xs:element name="getFullAnimalRegistrationByGuidRequest" type="hrt:GetFullAnimalRegistrationByGuidRequest"/>
    <xs:element name="getFullAnimalRegistrationByGuidResponse" type="hrt:GetFullAnimalRegistrationByGuidResponse"/>

    <!-- AMS.HRT.1.8 | Получение всей истории изменения сведений
                       о регистрации животного или группы животных по глобальному идентификатору -->
    <xs:element name="getAnimalRegistrationHistoryRequest" type="hrt:GetAnimalRegistrationHistoryRequest"/>
    <xs:element name="getAnimalRegistrationHistoryResponse" type="hrt:GetAnimalRegistrationHistoryResponse"/>

    <!-- AMS.HRT.1.9 | Получение списка изменений по основным сведениям
                       о регистрации животного или группы животных за указанный период времени -->
    <xs:element name="getAnimalRegistrationChangesListRequest" type="hrt:GetAnimalRegistrationChangesListRequest"/>
    <xs:element name="getAnimalRegistrationChangesListResponse" type="hrt:GetAnimalRegistrationChangesListResponse"/>

    <!-- AMS.HRT.1.10 | Получение редакции (версии) основных сведений
                        о регистрации животного или группы животных по идентификатору конкретной версии -->
    <xs:element name="getAnimalRegistrationByUuidRequest" type="hrt:GetAnimalRegistrationByUuidRequest"/>
    <xs:element name="getAnimalRegistrationByUuidResponse" type="hrt:GetAnimalRegistrationByUuidResponse"/>

    <!-- AMS.HRT.1.11 | Получение списка основных актуальных сведений
                        о регистрации животного или группы животных с заданными параметрами фильтрации -->
    <xs:element name="getAnimalRegistrationListRequest" type="hrt:GetAnimalRegistrationListRequest"/>
    <xs:element name="getAnimalRegistrationListResponse" type="hrt:GetAnimalRegistrationListResponse"/>

    <!-- AMS.HRT.1.12 | Получение редакции (версии) полных сведений (включая ветпрофмероприятия)
                        о регистрации животного или группы животных по идентификатору конкретной версии -->
    <xs:element name="getFullAnimalRegistrationByUuidRequest" type="hrt:GetFullAnimalRegistrationByUuidRequest"/>
    <xs:element name="getFullAnimalRegistrationByUuidResponse" type="hrt:GetFullAnimalRegistrationByUuidResponse"/>

    <!-- AMS.HRT.1.13 | Пополнение зарегистрированной группы животных -->
    <xs:element name="includeToAnimalGroupRequest" type="hrt:IncludeToAnimalGroupRequest"/>
    <xs:element name="includeToAnimalGroupResponse" type="hrt:IncludeToAnimalGroupResponse"/>

    <!-- AMS.HRT.1.14 | Исключение животных из зарегистрированной группы -->
    <xs:element name="excludeFromAnimalGroupRequest" type="hrt:ExcludeFromAnimalGroupRequest"/>
    <xs:element name="excludeFromAnimalGroupResponse" type="hrt:ExcludeFromAnimalGroupResponse"/>


    <!-- ==========================================================================================-->
    <!-- AMS.HRT.2 | Операции по работе со сведениями об идентификации животного (группы животных) -->
    <!-- ==========================================================================================-->

    <!-- AMS.HRT.2.1 |  Регистрация сведений об идентификации (маркирование)
                        зарегистрированного животного или группы животных -->
    <xs:element name="registerAnimalIdentityRequest" type="hrt:RegisterAnimalIdentityRequest"/>
    <xs:element name="registerAnimalIdentityResponse" type="hrt:RegisterAnimalIdentityResponse"/>

    <!-- AMS.HRT.2.2 |  Прекращение регистрации сведений об идентификации (снятие средства маркирования)
                        зарегистрированного животного или группы животных -->
    <xs:element name="removeAnimalIdentityRequest" type="hrt:RemoveAnimalIdentityRequest"/>
    <xs:element name="removeAnimalIdentityResponse" type="hrt:RemoveAnimalIdentityResponse"/>

    <!-- AMS.HRT.2.3 |  Перерегистрация сведений об идентификации (замена средства маркирования)
                        зарегистрированного животного или группы животных -->
    <xs:element name="replaceAnimalIdentityRequest" type="hrt:ReplaceAnimalIdentityRequest"/>
    <xs:element name="replaceAnimalIdentityResponse" type="hrt:ReplaceAnimalIdentityResponse"/>

    <!-- AMS.HRT.2.4 |  Уведомление о необходимости перерегистрации сведений об идентификации (замене средства маркирования)
                        зарегистрированного животного или группы животных -->
    <xs:element name="replaceAnimalIdentityNotificationRequest" type="hrt:ReplaceAnimalIdentityNotificationRequest"/>
    <xs:element name="replaceAnimalIdentityNotificationResponse" type="hrt:ReplaceAnimalIdentityNotificationResponse"/>

    <!-- AMS.HRT.2.5 |  Аннулирование сведений об идентификации (удаление средства маркирования)
                        зарегистрированного животного или группы животных -->
    <xs:element name="withdrawAnimalIdentityRequest" type="hrt:WithdrawAnimalIdentityRequest"/>
    <xs:element name="withdrawAnimalIdentityResponse" type="hrt:WithdrawAnimalIdentityResponse"/>

    <!-- AMS.HRT.2.6 |  Проверка статусов УНСМ (уникальных номеров средств маркирования) -->
    <xs:element name="verifyAnimalIDRequest" type="hrt:VerifyAnimalIDRequest"/>
    <xs:element name="verifyAnimalIDResponse" type="hrt:VerifyAnimalIDResponse"/>


    <!-- ===========================================================================================================-->
    <!-- AMS.HRT.3 | Операции по работе с ветеринарными профилактическими мероприятиями животного (группы животных) -->
    <!-- ===========================================================================================================-->

    <!-- AMS.HRT.3.1 |  Регистрация ветеринарного профилактического мероприятия -->
    <xs:element name="registerAnimalVeterinaryEventRequest" type="hrt:RegisterAnimalVeterinaryEventRequest"/>
    <xs:element name="registerAnimalVeterinaryEventResponse" type="hrt:RegisterAnimalVeterinaryEventResponse"/>

    <!-- AMS.HRT.3.2 |  Редактирование сведений о ветеринарном профилактическом мероприятии -->
    <xs:element name="modifyAnimalVeterinaryEventRequest" type="hrt:ModifyAnimalVeterinaryEventRequest"/>
    <xs:element name="modifyAnimalVeterinaryEventResponse" type="hrt:ModifyAnimalVeterinaryEventResponse"/>

    <!-- AMS.HRT.3.3 |  Подтверждение сведений о ветеринарном профилактическом мероприятии -->
    <xs:element name="confirmAnimalVeterinaryEventRequest" type="hrt:ConfirmAnimalVeterinaryEventRequest"/>
    <xs:element name="confirmAnimalVeterinaryEventResponse" type="hrt:ConfirmAnimalVeterinaryEventResponse"/>

    <!-- AMS.HRT.3.4 |  Аннулирование сведений о ветеринарном профилактическом мероприятии -->
    <xs:element name="withdrawAnimalVeterinaryEventRequest" type="hrt:WithdrawAnimalVeterinaryEventRequest"/>
    <xs:element name="withdrawAnimalVeterinaryEventResponse" type="hrt:WithdrawAnimalVeterinaryEventResponse"/>

    <!-- AMS.HRT.3.5 |  Получение актуальных сведений
                        о зарегистрированном ветеринарном профилактическом мероприятии по глобальному идентификатору -->
    <xs:element name="getAnimalVeterinaryEventByGuidRequest" type="hrt:GetAnimalVeterinaryEventByGuidRequest"/>
    <xs:element name="getAnimalVeterinaryEventByGuidResponse" type="hrt:GetAnimalVeterinaryEventByGuidResponse"/>

    <!-- AMS.HRT.3.6 |  Получение списка изменений сведений
                        о зарегистрированных ветеринарных профилактических мероприятий за указанный период времени -->
    <xs:element name="getAnimalVeterinaryEventChangesListRequest" type="hrt:GetAnimalVeterinaryEventChangesListRequest"/>
    <xs:element name="getAnimalVeterinaryEventChangesListResponse" type="hrt:GetAnimalVeterinaryEventChangesListResponse"/>

    <!-- AMS.HRT.3.7 |  Получение всей истории изменений сведений
                        о зарегистрированном ветеринарном профилактическом мероприятии по глобальному идентификатору -->
    <xs:element name="getAnimalVeterinaryEventHistoryRequest" type="hrt:GetAnimalVeterinaryEventHistoryRequest"/>
    <xs:element name="getAnimalVeterinaryEventHistoryResponse" type="hrt:GetAnimalVeterinaryEventHistoryResponse"/>

    <!-- AMS.HRT.3.8 |  Получение редакции (версии) сведений
                        о зарегистрированном ветеринарном профилактическом мероприятии по идентификатору версии -->
    <xs:element name="getAnimalVeterinaryEventByUuidRequest" type="hrt:GetAnimalVeterinaryEventByUuidRequest"/>
    <xs:element name="getAnimalVeterinaryEventByUuidResponse" type="hrt:GetAnimalVeterinaryEventByUuidResponse"/>


    <!-- Application types -->

    <!-- Базовый тип любого запроса, включающий в себя сведения об инициаторе -->
    <xs:complexType name="HerriotApplicationRequest">
        <xs:complexContent>
            <xs:extension base="apl:ApplicationData">
                <xs:sequence>
                    <xs:element name="localTransactionId" type="bs:UUID" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="initiator" type="vd:User" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="sessionToken" type="dt:OTPToken" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <!-- ===============================================================================-->
    <!-- AMS.HRT.1 | Операции по работе с учетной карточкой животного (группы животных) -->
    <!-- ===============================================================================-->

    <xs:complexType name="RegisterAnimalRequest">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.1</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Запрос: Регистрация животного или группы животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="hrt:HerriotApplicationRequest">
                <xs:sequence>
                    <xs:element name="animalRegistration" type="vd:AnimalRegistration" minOccurs="1" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                Проект учётной карточки животного или группы животных
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="RegisterAnimalResponse">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.1</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Ответ: Регистрация животного или группы животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="apl:ApplicationResultData">
                <xs:sequence>
                    <xs:element name="animalRegistration" type="vd:AnimalRegistration" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                Зарегистрированная учётная карточка животного или группы животного
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ModifyAnimalRegistrationRequest">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.2</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Запрос: Внесение изменений в основные сведения о зарегистрированном животном или группе животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="hrt:HerriotApplicationRequest">
                <xs:sequence>
                    <xs:element name="animalRegistration" type="vd:AnimalRegistration" minOccurs="1" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                Проект изменений существующей учётной карточки с указанным уникальным идентификатором версии
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ModifyAnimalRegistrationResponse">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.2</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Ответ: Внесение изменений в основные сведения о зарегистрированном животном или группе животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="apl:ApplicationResultData">
                <xs:sequence>
                    <xs:element name="animalRegistration" type="vd:AnimalRegistration" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                Обновленная учётная карточка животного или группы животных
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RegisterAnimalMovementEventRequest">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.3</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Запрос: Регистрация сведений о перемещении / смене собственника зарегистрированного животного или групп животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="hrt:HerriotApplicationRequest">
                <xs:sequence>
                    <xs:element name="referencedMovementEvent" type="vd:AnimalMovementEvent" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Сведения о перемещении
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="movementAnimalRegistrationList" type="vd:AnimalRegistrationList" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Cведения о перемещаемых зарегистрированных животных или групп животных
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="RegisterAnimalMovementEventResponse">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.3</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Ответ: Регистрация сведений о перемещении / смене собственника зарегистрированного животного или групп животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="apl:ApplicationResultData">
                <xs:sequence>
                    <xs:element name="movementAnimalRegistrationList" type="vd:AnimalRegistrationList" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Список учётных карточек перемещаемых животных или групп животных
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="referencedAnimalRegistrationList" type="vd:AnimalRegistrationList" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Список связанных карточек животных, которые в явном виде не участвовали в запросе, но были
                                изменены в результате выполнения операции (например, перемещение индивидуального животного,
                                входящего в группу, влечёт изменение данной группы).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="TerminateAnimalRegistrationRequest">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.4</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Запрос: Прекращение регистрации (выбытие) животного или группы животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="hrt:HerriotApplicationRequest">
                <xs:sequence>
                    <xs:element name="referencedLifecycleEvent" type="vd:AnimalLifecycleEvent" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Сведения о выбытии животного или группы животных
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="animalRegistrationRef" type="vd:AnimalRegistration" minOccurs="1" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                Учётная карточка животного или группы животных, в отношении которой регистрируется выбытие
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TerminateAnimalRegistrationResponse">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.4</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Ответ: Прекращение регистрации (выбытие) животного или группы животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="apl:ApplicationResultData">
                <xs:sequence>
                    <xs:element name="terminatedAnimalRegistrationList" type="vd:AnimalRegistrationList" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Список учётных карточек выбывших животных или групп животных
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="referencedAnimalRegistrationList" type="vd:AnimalRegistrationList" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Список связанных карточек животных, которые в явном виде не участвовали в запросе, но были
                                изменены в результате выполнения операции (например, обновление группы животных,
                                в которой содержалось выбывшее индивидуальное животное из запроса).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="WithdrawAnimalRegistrationRequest">
        <xs:annotation>
            <xs:appinfo>
                <hrt:operationID>AMS.HRT.1.5</hrt:operationID>
            </xs:appinfo>
            <xs:documentation>
                Запрос: Аннулирование сведений о регистрации животного или группы животных
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="hrt:HerriotApplicationRequest">
                <xs:sequence>
                    <xs:element name="withdrawDetails" type="vd:AnimalRegistrationStatusChange" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation>
                                Сведения об аннулировании учётной карточки животного
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="animalRegistrationRef" type="vd:AnimalRegistration" minOccurs="1" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                Учётная карточка животного или группы животных, в отношении которой регистрируется аннулирование
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:any minOccurs="0" maxOccurs="unbounded" namespace="##targetNamespace"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="WithdrawAnimalRegistrationResponse">
        <xs:annotation>
